{
    "name": "lcobucci/jwt",
    "description": "A simple library to work with JSON Web Token and JSON Web Signature",
    "type": "library",
    "authors": [
        {
            "name": "Lu√≠s Cobucci",
            "email": "lcobucci@gmail.com",
            "role": "Developer"
        }
    ],
    "keywords": [
        "JWT",
        "JWS"
    ],
    "license": [
        "BSD-3-Clause"
    ],
    "require": {
        "php": "^7.1",
        "ext-openssl": "*",
        "lcobucci/jose-parsing": "~2.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^2.7",
        "phpmd/phpmd": "^2.5",
        "phpunit/php-invoker": "^1.1",
        "mikey179/vfsStream": "^1.6",
        "mdanter/ecc": "^0.4",
        "humbug/humbug": "dev-master@dev"
    },
    "suggest":{
        "mdanter/ecc": "Required to use Elliptic Curves based algorithms."
    },
    "autoload": {
        "psr-4": {
            "Lcobucci\\JWT\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lcobucci\\JWT\\": [
                "test/_keys",
                "test/unit",
                "test/functional"
            ]
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.0-dev"
        }
    }
}
